<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="io.volcain.android.birdie">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".SongsActivity"
            android:label="@string/category_songs" />
        <activity
            android:name=".ArtistsActivity"
            android:label="@string/category_artists" />
        <activity
            android:name=".GenreActivity"
            android:label="@string/category_genre" />
        <activity
            android:name=".AlbumsActivity"
            android:label="@string/category_albums" />
    </application>

</manifest>